			<outline text="December 31" created="Thu, 31 Dec 2020 14:45:34 GMT" type="calendarDay" name="31">
				<outline text="See you next year! :boom:" created="Fri, 01 Jan 2021 04:01:38 GMT" type="outline" inlineImage="http://scripting.com/images/2020/12/31/thatsAllFolks.png"/>
