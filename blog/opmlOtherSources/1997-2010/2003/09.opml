				<outline text="&lt;rules&gt;" created="Mon, 29 Sep 2003 05:02:21 GMT">
					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;">
						<outline text="&lt;internalLink&gt;http://scriptingnews.userland.com/2003/09/29&lt;/internalLink&gt;"/>
