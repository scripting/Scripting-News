				<outline text="Chris Lydon &lt;a href=&quot;http://blogs.law.harvard.edu/lydon/2003/10/03#a360&quot;&gt;interviewed&lt;/a&gt; Adam Curry yesterday. &lt;i&gt;Bing!&lt;/i&gt; &quot;;-&gt;&quot;" created="Sat, 04 Oct 2003 09:37:58 GMT" type="outline"/>
				<outline text="Before 8AM Eastern, the &lt;a href=&quot;http://blogs.law.harvard.edu/bloggerCon/webcast&quot;&gt;webcast page&lt;/a&gt; will point to the webcast." created="Sat, 04 Oct 2003 09:44:50 GMT" type="outline"/>
				<outline text="&lt;rules&gt;" created="Sat, 04 Oct 2003 05:23:18 GMT">
					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;">
						<outline text="&lt;textStyle&gt;bold&lt;/textStyle&gt;"/>
