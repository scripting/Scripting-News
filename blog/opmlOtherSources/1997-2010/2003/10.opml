				<outline text="&lt;rules&gt;" created="Sat, 18 Oct 2003 05:02:40 GMT">
					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;">
						<outline text="&lt;internalLink&gt;http://scriptingnews.userland.com/2003/10/18&lt;/internalLink&gt;"/>
						<outline text="&lt;/rule&gt; "/>
