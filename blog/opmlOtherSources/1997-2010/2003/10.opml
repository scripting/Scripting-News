				<outline text="&lt;rules&gt;" created="Wed, 29 Oct 2003 05:02:22 GMT">
					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;">
						<outline text="&lt;internalLink&gt;http://blogs.law.harvard.edu/scriptingArchive/2003/10/29&lt;/internalLink&gt;"/>
