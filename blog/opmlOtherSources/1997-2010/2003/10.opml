			<outline text="October 18" created="Sat, 18 Oct 2003 05:00:01 GMT">
				<outline text="&lt;rules&gt; " created="Sat, 18 Oct 2003 05:01:25 GMT">
					<outline text="&lt;rule&gt;&lt;outlineSpacing&gt;1.5&lt;/outlineSpacing&gt;&lt;/rule&gt;"/>
