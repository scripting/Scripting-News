				<outline text="&lt;rules&gt;" created="Thu, 22 Jun 2000 05:02:35 GMT">
					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;">
						<outline text="&lt;textStyle&gt;bold&lt;/textStyle&gt;"/>
						<outline text="&lt;internalLink&gt;http://scriptingnews.userland.com/backissues/2000/06/22&lt;/internalLink&gt;"/>
