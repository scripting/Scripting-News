				<outline text="&lt;rules&gt;" created="Sat, 10 Jun 2000 05:02:17 GMT">
					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;&lt;textStyle&gt;bold&lt;/textStyle&gt;&lt;/rule&gt;"/>
					<outline text="&lt;/rules&gt;"/>
