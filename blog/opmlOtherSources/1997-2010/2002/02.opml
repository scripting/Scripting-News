					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;">
						<outline text="&lt;internalLink&gt;http://scriptingnews.userland.com/backissues/2002/02/14&lt;/internalLink&gt;"/>
						<outline text="&lt;/rule&gt; "/>
						</outline>
