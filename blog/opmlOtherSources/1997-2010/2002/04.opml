					<outline text="&lt;/rules&gt; "/>
					</outline>
				<outline text="&lt;rules&gt;">
					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;">
						<outline text="&lt;internalLink&gt;http://scriptingnews.userland.com/backissues/2002/04/05&lt;/internalLink&gt;"/>
						<outline text="&lt;/rule&gt; "/>
