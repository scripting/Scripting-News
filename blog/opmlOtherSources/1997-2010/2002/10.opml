				<outline text="&lt;rules&gt;" created="Wed, 16 Oct 2002 05:02:02 GMT">
					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;">
						<outline text="&lt;internalLink&gt;http://scriptingnews.userland.com/backissues/2002/10/16&lt;/internalLink&gt;"/>
						<outline text="&lt;/rule&gt; "/>
