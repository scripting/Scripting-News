			<outline text="June 11" created="Tue, 11 Jun 2002 05:00:01 GMT">
				<outline text="&lt;rules&gt; " created="Tue, 11 Jun 2002 05:01:19 GMT">
					<outline text="&lt;rule&gt;&lt;outlineSpacing&gt;1.5&lt;/outlineSpacing&gt;&lt;/rule&gt;"/>
					<outline text="&lt;/rules&gt; "/>
					</outline>
				<outline text="&lt;rules&gt;" created="Tue, 11 Jun 2002 05:02:52 GMT">
					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;">
						<outline text="&lt;internalLink&gt;http://scriptingnews.userland.com/backissues/2002/06/11&lt;/internalLink&gt;"/>
						<outline text="&lt;/rule&gt; "/>
